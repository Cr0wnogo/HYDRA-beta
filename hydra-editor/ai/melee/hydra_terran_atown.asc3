	:addBunker
	var Ex|wait canStatic 1 13
	wait 12
	notowns barracks addBunker
	call wUpgradeDef
	res 13 AtL_C Ore 125 addBunkerCheck
	wait 720
	goto addBunker
	
	:addBunkerCheck
	add_bld 255 bunker 1 1
	local 255 AtL 1 bunker addReturn
	p_guard marine 0
	p_guard marine 0
	p_guard marine 1
	p_guard firebat 0
	goto addReturn
	
	:addTurret
	wait 12
