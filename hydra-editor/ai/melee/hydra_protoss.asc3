	:uAttack
	wait 12
	player_combat 17 13 63 True Wait Any Group_Buildings|probe
	var set underAttack 1 13
	under_attack 1
	player_combat 17 13 63 False WF(240) Any Group_Buildings|probe
	wait 240
	call uAttackMax
	
	:uAttackFin
	var set underAttack 0 13
	under_attack 0
	goto uAttack
	
	:uAttackMax
	under_attack 2
	player_combat 17 13 63 False WF(480) Any Group_Buildings|probe
	return
	
	:wAttack
	wait 24
	bring 13 AtM 12 probe Loc.63 wAttack
	bring 13 AtM 1 gateway|principality|warp_anchor|robotics_facility|robotics_authority|stargate|argosy|starfield Loc.63 wAttack
	bring 13 AtL 10 Group_Factories Loc.63 blankReturn
	res 13 AtL Any 999 blankReturn
	var Ex|WF(360) underAttack 0 13
	res 13 AtM Ore 199 wAttack
	return
	
